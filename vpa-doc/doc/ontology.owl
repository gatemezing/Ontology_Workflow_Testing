  <owl:ObjectProperty rdf:about="http://data.europa.eu/949/areaOfUse">
    <rdfs:domain>
      <owl:Class rdf:about="http://data.europa.eu/949/vpa/VehicleTypeAuthorisationCase">
        <rdfs:subClassOf>
          <owl:Class rdf:about="http://data.europa.eu/949/vpa/Case">
            <rdfs:comment xml:lang="en">A clearly separated part of a Request, identified clearly by its Scope(s). Each Case has an individual PermissionType as objective. A Request consists out of minimally one Case.</rdfs:comment>
            <rdfs:label xml:lang="en">Case</rdfs:label>
          </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The authorisation case as defined in Article 14 of https://eur-lex.europa.eu/eli/reg_impl/2018/545/oj. The scope of the case is therefore always (1) one of the 5 PermissionTypes as in the Article 14, (2) the unique vehicle type identifier and (3) the set of vehicles belonging to the Authorisation Case.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://eur-lex.europa.eu/eli/reg_impl/2018/545/oj"/>
        <rdfs:label xml:lang="en">Vehicle Type Authorisation Case</rdfs:label>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept">
      </owl:Class>
    </rdfs:range>
    <rdfs:comment xml:lang="en">Member State networks, or parts thereof, which were analysed during the (vehicle(type)) authorisation process.</rdfs:comment>
    <rdfs:label xml:lang="en">area of use</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://data.europa.eu/949/hasOrganisationRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class rdf:about="http://data.europa.eu/949/OrganisationRole">
        <owl:disjointWith>
          <owl:Class rdf:about="http://www.w3.org/ns/org#Organization">
          </owl:Class>
        </owl:disjointWith>
        <rdfs:label xml:lang="en">Organisation Role</rdfs:label>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">has organisation role</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://data.europa.eu/949/role">
    <rdfs:domain rdf:resource="http://www.w3.org/ns/org#Organization"/>
    <rdfs:range rdf:resource="http://data.europa.eu/949/OrganisationRole"/>
    <rdfs:label xml:lang="en">role</rdfs:label>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://data.europa.eu/949/vpa/">
    <cc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
