        <rdfs:comment xml:lang="en">valid during a certain time</rdfs:comment>
        <rdfs:label xml:lang="en">valid</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/vpa#withRestriction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/vpa#withRestriction">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/vpa#Compliance"/>
